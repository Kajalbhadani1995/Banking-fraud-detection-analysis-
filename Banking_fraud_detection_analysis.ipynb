   "source": [
    "def risk_category(amount):\n",
    "    if amount <= 10000:\n",
    "        return \"Low Risk\"\n",
    "    elif amount <= 50000:\n",
    "        return \"Medium Risk\"\n",
    "    else :\n",
    "        return \"High Risk\"\n",
    "\n",
    "df[\"transaction_risk\"] = df[\"transaction_amount\"].apply(risk_category)\n",
    "        "
   ]
  },
